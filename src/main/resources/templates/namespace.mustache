<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
  <head>
    <title>Emissary - Namespace Listing</title>
    <link type="text/css" rel="stylesheet" href="/css/emissary.css">
  </head>

  <body>
    <h1>Namespace Listing</h1>

    <table class="datagrid">
       <thead>
          <tr><th>Key</th><th>Value</th></tr>
        </thead>
        <tbody>
          {{#namespaces}}
          <tr class='{{clz}}'>
            {{#isDir}}
              <td>
                <a href="{{dumpUrl}}">{{key}}</a>
                (<a href="{{transferUrl}}">xml</a>)
              </td>
              <td>
                {{value}}({{valueClassName}})
              </td>
            {{/isDir}}
            {{^isDir}}
                <td>{{key}}</td>
                <td>
                  {{value}}({{valueClassName}})
                </td>
            {{/isDir}}
          </tr>
          {{/namespaces}}
       </tbody>
     </table>
  </body>
</html>
